  <system.serviceModel>
    <client/>
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true" />
    <bindings>
      <basicHttpBinding>
        <binding name="VentusDataServiceBinding" maxBufferSize="2147483647" maxReceivedMessageSize="2147483647" maxBufferPoolSize="20000000" receiveTimeout="00:30:00" sendTimeout="00:30:00">
                  <readerQuotas maxDepth="32" maxArrayLength="200000000" maxStringContentLength="200000000"/>
        </binding>
      </basicHttpBinding>
	  
      <wsHttpBinding>
        <binding name="VentusDataServiceBinding" maxReceivedMessageSize="2147483647" maxBufferPoolSize="20000000" receiveTimeout="00:30:00" sendTimeout="00:30:00">
            <readerQuotas maxDepth="32" maxArrayLength="200000000" maxStringContentLength="200000000"/>
        </binding>
        <!-- invalid value from kent: maxReceivedMessageSize="655360000" -->
      </wsHttpBinding>
    </bindings>
    
    
    
    <services>
      <service behaviorConfiguration="VentusDataServiceBehavior" name="Ventus.API.v1000.helperService">
        <endpoint binding="basicHttpBinding" bindingConfiguration="VentusDataServiceBinding" contract="Ventus.API.v1000.IhelperService" />
      </service>

      <service behaviorConfiguration="VentusDataServiceBehavior" name="Ventus.API.v1000.OModelService">
        <endpoint binding="basicHttpBinding" bindingConfiguration="VentusDataServiceBinding" contract="Ventus.API.v1000.IOModelService" />
      </service>

	  
	  <service behaviorConfiguration="VentusDataServiceBehavior" name="Ventus.API.v1000.testingService">
        <endpoint binding="basicHttpBinding" bindingConfiguration="VentusDataServiceBinding" contract="Ventus.API.v1000.ItestingService" />
      </service>
      
      <service behaviorConfiguration="VentusDataServiceBehavior" name="Ventus.API.v1000.moduleWF">
