	move &x.xrhsh_arxh to $xrhsh_arxh
from ap_xrhsh x
where cid = $cid
and $hmer_apo between xrhsh_arxh and xrhsh_telos
End-Select

move 0 to #hdlr
!move 218149 to #hdlr

execute -C1 whp_tmpapog 'SQR', #hdlr output,$cid, $hmer_apo, $hmer_evs, $apo_eidos, $evs_eidos

display 'HDLR ' noline
display #hdlr

End-Procedure

