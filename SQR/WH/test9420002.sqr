begin-procedure main2
  
begin-select distinct
a.kvdikos &eidos () NOP

 let $eidos     = &eidos
 let $omada_eid = ''
 let $per_eid   = ''
 
 do get_omadaeid
 do insert2

 let #cnt=#cnt+1

from   wh_eidhinf a, wh_eidhmore b
where  a.cid     =     '1' 
and    a.cid     =      b.cid 
and    a.inf     =      $omada 
and    a.kvdikos =      b.eidos
and    b.energo  is not null 
and    b.logar   is     null
! v   Mal Nick
and    a.kvdinf  = 200
! ^   Mal Nick
