  do main ($apohm, $evshm, $prehm, &tmhma, $deigma, #tot_apo_pos, #tot_ypo_pos, #tot_pre_ypo_pos, #tot_pre_mes_tim, #tot_pre_kos_ypo, #tot_ypo_aji_ago, #tot_ypo_aji_pvl, #tot_kos_ypo, #tot_kos_pvl, #tot_mik_ker_aji, #tot_mik_ker_pos)

from ap_tmhma a
where 1=1
[$SQLCMD]
order by
[$SQLORD]
end-select

 if #tot_kos_pvl <> 0
