 print #tot_ypo_aji_ago   (,+1,13) edit {MASK2-0}
 print #tot_kos_ypo       (,+1,13) edit {MASK2-0}
 print #tot_ypo_aji_pvl   (,+1,13) edit {MASK2-0}
