! ******************************************************************************
begin-procedure deigma($e,$apohm,$evs,$pre,:#d_pos)

  let  #d_pos = 0
  let  #d_pos21 = 0
  let  #d_pos26 = 0

begin-select
a.posothta    &d_posothta () NOP
a.parkvd      &d_parkvd   () NOP

  let #d_po = &d_posothta
  let $d_pa = &d_parkvd
  
  if $d_pa = '21'
     let #d_pos21 = #d_pos21 + #d_po
  end-if
  
  if $d_pa = '26'
     let #d_pos26 = #d_pos26 + (#d_po * -1)
  end-if

from wh_kin a,sy_parmas b,wh_eidh c
where a.aa       = b.aa
and   c.pvlomada = $e
and   a.eidos    = c.kvdikos
and   b.skopos   = 3 
and   a.parkvd   in ('21','26')
and   a.hmeromhnia between $apohm and $evs
end-select
! display #d_pos21 noline
! display '  '     noline
! display #d_pos26 noline
     let #d_pos = #d_pos21 - #d_pos26
! display ' '      noline

end-procedure

