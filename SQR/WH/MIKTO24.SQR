  let #tmh_ypo_aji_pvl   = #tmh_ypo_aji_pvl + #ypo_aji_pvl
  let #tmh_kos_ypo       = #tmh_kos_ypo + #kos_ypo
  let #tmh_kos_pvl       = #tmh_kos_pvl + #kos_pvl
  let #tmh_mik_ker_aji   = #tmh_mik_ker_aji + #mik_ker_aji

