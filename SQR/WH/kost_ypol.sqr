begin-procedure akostologhta1
  let #tim_ky     =  0
  let #tim_pos    =  0   
begin-select
d.pvlomada      &eidos1 () NOP
sum(b.posothta) &pos1   () NOP

        let $pvl         = &eidos1
        let #tim_pos     = &pos1
        let #tim_ky      = #tim_pos       * #msk
        let #tot_tim_pos = #tot_tim_pos   + #tim_pos     
        let #tot_tim_ky  = #tot_tim_ky    + #tim_ky

!  do getmsk


from  sy_parsxet a, wh_kin b, wh_kin c, wh_eidh d
where a.link_aa   = b.aa
and   a.parkvd    = '13'
and   d.kvdikos   = b.eidos
and   a.aa        = c.aa
and   c.hmeromhnia > $evshm       
and   b.hmeromhnia between $start_hmxrhsh and $evshm
and   b.eidos      = c.eidos
and   b.cid        = '1'
and   b.cid        = c.cid
and   c.cid        = '1'
and   d.pvlomada   = $pvl
group by d.pvlomada
end-select
end-procedure                      

! *************************************************************************

begin-procedure akostologhta2
  let #ntim_ky     =  0
  let #ntim_pos    =  0   
begin-select
d.pvlomada      &eidos2 () NOP
sum(b.posothta) &pos2   () NOP

        let $pvl           = &eidos2
        let #ntim_pos      = &pos2
        let #ntim_ky       = #ntim_pos      * #msk
        let #tot_ntim_pos  = #tot_ntim_pos  + #ntim_pos     
        let #tot_ntim_ky   = #tot_ntim_ky   + #ntim_ky

!  do getmsk

from  wh_eidh d,wh_akostol b
where d.kvdikos = b.eidos
and   b.hmeromhnia between $start_hmxrhsh and $evshm
and   b.cid = '1'
and   d.pvlomada = $pvl
group by d.pvlomada
end-select
end-procedure                      

! *************************************************************************

begin-procedure getmsk
  let $cid = '1'
  let #ago_a = 0
  let #pvl_a = 0
  let #ago_p = 0
  let #pvl_p = 0
  execute whp_getmsk7 
                         @cid         = $cid, 
                         @eidos       = $pvl,
                         @hmer        = $evshm,  
                         @msk         = #msk    out,
                         @ypoloipo    = #pl_pos out,
                         @pvl_p       = #pvl_p  out,
                         @pvl_a       = #pvl_a  out,
                         @ago_p       = #ago_p  out,
                         @ago_a       = #ago_a  out 

  let #msk = round(#msk,0)

end-procedure                      

