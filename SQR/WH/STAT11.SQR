Begin-Procedure do_it2
  let #cnt_synal = 1
  next-listing
          print ' '                 (, 1,       20) 
          print ' '                 (,+1,       20) 
          print ' '                 (,+1,       17) 
          print '-'                 (,+1,#POS_CHAR) fill 
          print ' '                 (,+1,#POS_CHAR) 
