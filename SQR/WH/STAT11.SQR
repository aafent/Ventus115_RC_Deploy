Begin-Procedure find_tmhma
begin-select
a.perigrafh  &tmhma_per  () NOP
  let $tmhma_per = &tmhma_per
from  ap_tmhma a
where a.kvdikos = #tmhma
end-select
End-Procedure
! ==============================================================================
Begin-Procedure find_synal
  next-listing
begin-select
a.epvnymia      &epvn      () NOP
  
  let $epvnymia  = &epvn     
from  sy_synal a
where a.synal = $synal
end-select
  print $synal       (,20,10) 
  print $epvnymia    (,31,30)
  next-listing
!  do   do_it2 
End-Procedure
! ==============================================================================

begin-procedure find_etaireia
begin-select
a.l1          &per1 () NOP
a.l2          &per2 () NOP
  let $p_et1 = &per1
  let $p_et2 = &per2
  display $p_et1
  display $p_et2
from ap_comp a
where a.cid = $cid
end-select
end-procedure
