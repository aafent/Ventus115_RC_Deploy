Begin-Procedure do_it

          print ' '                 (, 1,       20) 
          print ' '                 (,+1,       20) 
          print ' '                 (,+1,       17) 
          print '-'                 (,+1,#POS_CHAR) fill 
          print ' '                 (,+1,#POS_CHAR) 
          print ' '                 (,+1,#POS_PERC) 
          print '-'                 (,+1,#AJI_CHAR) fill 
          print ' '                 (,+1,#AJI_CHAR) 
          print ' '                 (,+1,#POS_PERC) 
  next-listing
          print ' '                 (, 1,       20) 
          print ' '                 (,+1,       20) 
          print '”’ÕœÀ¡ –œÀ«”  :'   (,+1,       17) 
          print #s_pos              (,+1,#POS_CHAR)  EDIT {MASK7}
          print ' '                 (,+1,#POS_CHAR) 
          print ' '                 (,+1,#POS_PERC) 
          print #s_ajia             (,+1,#AJI_CHAR)  EDIT {MASK7}
          print ' '                 (,+1,#AJI_CHAR) 
          print ' '                 (,+1,#POS_PERC) 
  
 next-listing 
 next-listing 
 new-page
 let #s_pos     = 0
 let #s_ajia    = 0
 let #cnt       = 1
 let #count     = 1
End-Procedure
