Begin-Procedure do_it3
          print ' '                 (, 1,       20) 
          print ' '                 (,+1,       20) 
          print ' '                 (,+1,       17) 
          print '-'                 (,+1,#POS_CHAR) fill 
          print ' '                 (,+1,#POS_CHAR) 
          print ' '                 (,+1,#POS_PERC) 
          print '-'                 (,+1,#AJI_CHAR) fill 
          print ' '                 (,+1,#AJI_CHAR) 
          print ' '                 (,+1,#POS_PERC) 
  next-listing
          print ' '                 (, 1,       20) 
          print ' '                 (,+1,       20) 
          print '”’ÕœÀ¡ ‘Ã«Ã¡‘œ”:'  (,+1,       17) 
          print #tmhma_pos          (,+1,#POS_CHAR)  EDIT {MASK7}
          print ' '                 (,+1,#POS_CHAR) 
          print ' '                 (,+1,#POS_PERC) 
          print #tmhma_ajia         (,+1,#AJI_CHAR)  EDIT {MASK7}
          print ' '                 (,+1,#AJI_CHAR) 
          print ' '                 (,+1,#POS_PERC) 
  next-listing
  next-listing
  let   #cnt          = 1
  let   #tmhma_ajia   = 0
  let   #tmhma_pos    = 0  
End-Procedure
! ------------------------------------------------------------------------------
Begin-Procedure do_it4
        move $pre_pos  to #pre_pos
        move $pre_ajia to #pre_ajia
End-Procedure
