begin-procedure getsxet(#aa, :$sx_seira, :$sx_parno)

  move '' to $sx_seira
  move '' to $sx_parno

begin-select
tim_seira                   () NOP
tim_parno                   () NOP

    move &tim_seira to $sx_seira 
    move &tim_parno to $sx_parno 99999

