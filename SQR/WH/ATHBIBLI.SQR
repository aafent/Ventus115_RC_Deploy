begin-procedure apografh
 
 execute app_geteom $startdate, $startdate_eom out

begin-select
sum(apo_p) &apo_qt       () NOP
sum(apo_a) &apo_aj       () NOP

  move &apo_qt to #apo_qt
  move &apo_aj to #apo_aj

from  wh_kint1
where cid         = $cid
and   apouhkh     = $apouhkh
and   eidos       = $eidos
and   hmeromhnia  = $startdate_eom
end-select

end-procedure

