/** PCA*LIB AdHoc-Reports **/
/** Saved : 2017-09-15 **/
SELECT e.kvdikos    AS sku,
	   e.perigrafh  AS desc1,
	   e.perigrafh2 AS desc2,
	   o.perigrafh  AS productCategory,
	   e.kathgoria
FROM wh_eidh e
LEFT JOIN wh_pvlomades o ON (e.pvlomada = o.pvlomada )
WHERE e.energo IS NOT NULL
AND o.is_active=1
