/** PCA*LIB AdHoc-Reports **/
/** Saved : 2012-01-05 **/
select a.aa, b.inx, a.seira,a.parno,a.hmer_e, b.eidos, e.perigrafh, b.posothta, round(b.ajia/b.posothta,4) as timh, b.ajia, b.ekp_p, b.extra_n1
from
sy_parmas a, sy_pardet b, wh_eidh e
where a.cid='10'
and a.kat='10'
and a.apouhkh='10'
and a.hmer_e between '1 jan 2010' and '31 dec 2010'
and a.parkvd = '221'
and a.synal='0010'
and a.aa = b.aa
and b.eidos = e.kvdikos
order by a.hmer_e
