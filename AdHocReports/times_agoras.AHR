/** PCA*LIB AdHoc-Reports **/
/** Saved : 2010-01-18 **/
select
e.kvdikos,e.perigrafh, m.synal, s.epvnymia, convert(varchar(10),m.hmer_e,103) as shmer_e, d.ajia, d.posothta,   m.aa
from wh_eidh e, sy_pardet d, sy_parmas m, sy_synal s
where e.kvdikos = d.eidos
and d.aa = m.aa
and m.parkvd='Á01'
and m.synal = s.synal
order by d.aa asc, m.hmer_e desc
