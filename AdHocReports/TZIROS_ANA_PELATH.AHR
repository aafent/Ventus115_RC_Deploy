/** PCA*LIB AdHoc-Reports **/
/** Saved : 2005-03-24 **/
select sum( a.ajia * c.syntel_xp * c.syntel_a )
from sy_partotals a, sy_parmas b, ap_par c, sy_synal d
where a.aa = b.aa
and b.parkvd = c.parkvd
and a.kvdikos = 1
and b.parkvd in (select parkvd from ap_omadapar where omada='PVLHSEIS')
and b.synal = d.synal
and b.hmer_e between '1 jan 2004' and '31 dec 2004'
and b.synal = '0001'
group by d.synal
