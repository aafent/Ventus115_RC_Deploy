/** PCA*LIB AdHoc-Reports **/
/** Saved : 2015-07-15 **/
SELECT
convert(VARCHAR(10),a.hmer_e,103) AS hmer, a.aa, a.parkvd,a.seira,a.parno,a.synal, b1.epvnymia,
convert(VARCHAR(10),a.s_hmer_e,103) AS s_hmer, a.s_aa,
a.s_parkvd,a.s_seira,a.s_parno,a.s_synal, b2.epvnymia
FROM sy_parsxetv1 a, sy_synal00 b1, sy_synal00 b2
WHERE a.synal = b1.synal
AND a.s_synal = b2.synal
AND a.hmer_e BETWEEN '1 jan 2014' AND '31 dec 2014'
